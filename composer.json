{
    "name": "sicet7/php-rr-server-plugin",
    "description": "Container plugin for a RoadRunner HTTP PSR-7 server setup",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-zip": "*",
        "ext-sockets": "*",
        "psr/http-server-handler": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^3.0",
        "spiral/roadrunner": "^2.12",
        "php-di/php-di": "^7.0",
        "psr/container": "^2.0",
        "sicet7/php-plugin-container": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sicet7\\Server\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Martin René Sørensen",
            "email": "git@sicet7.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:sicet7/php-plugin-container.git"
        }
    ],
    "minimum-stability": "stable"
}
